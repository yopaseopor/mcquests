          "en": "Type",
          "es": "Tipo",
          "ca": "Tipus"
        },
        "mappings": [
          {
            "if": {
              "and": [
                "survey:date~*"
              ]
            },
            "then": {
              "en": "Survey date:{survey:date}",
              "es": "Fecha de revisión:{survey:date}",
              "ca": "Data de revisió:{survey:date}"
            }
          },
          {
            "if": {
              "and": [
                "tracktype=grade1"
              ]
            },
            "then": {
              "en": "Pavemented",
              "es": "Pavimentado",
              "ca": "Pavimentat"
            }
          },
          {
            "if": {
              "and": [
                "tracktype=grade2"
              ]
            },
            "then": {
              "en": "No pavement",
              "es": "Sin pavimentar",
              "ca": "Sense pavimentar"
            }
          },
          {
            "if": {
              "and": [
                "tracktype=grade3"
              ]
            },
            "then": {
              "en": "Only 4x4",
              "es": "Sólo 4x4",
              "ca": "Només 4x4"
            }
          },
          {
            "if": {
              "and": [
                "survey:date="
              ]
