              "then": {
                "en": "Unpaved (grade2)",
                "es": "Sin pavimentar (grade2)",
                "ca": "No pavimentat (grade2)"
              }
            },
            {
              "if": {
                "and": [
                  "tracktype=grade3"
                ]
              },
              "then": {
                "en": "Only 4x4 (grade3)",
                "es": "Sólo 4x4 (grade3)",
                "ca": "Només 4x4 (grade3)"
              }
            }
          ]
        },
        {
          "render": {
            "en": "Surveyed on:{survey:date}",
            "es": "Comprobado el:{survey:date}",
