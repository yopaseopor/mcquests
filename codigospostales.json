          {
            "if": {
              "and": [
                "postal_code~"
              ]
            },
            "then": {
              "es": "#00f"
            }
          },
          {
            "if": {
              "and": [
                "postal_code="
              ]
            },
            "then": {
              "es": "#ff0000"
            }
          }
        ]
